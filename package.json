{
  "name": "angular-seed",
  "version": "1.0.0",
  "description": "angular seed project using webpack and ng4",
  "main": "index.js",
  "scripts": {
    "clean:dist": "rimraf ./dist",
    "coverage": "set CODE_COVERAGE=true & npm run test -- --browsers PhantomJS",
    "doc": "compodoc --tsconfig tsconfig.json --name 'angular-seed' --theme 'Readthedocs'",
    "doc:serve": "compodoc --serve --port 8090",
    "lint": "tslint 'src/**/*.ts' ",
    "serve:dev": "set NODE_ENV=development & webpack-dev-server --inline --progress --port 8080",
    "show:doc": "npm run doc && npm run doc:serve",
    "test": "karma start ./karma/karma.conf.js",
    "test:once": "set CODE_COVERAGE=false & npm run test -- --browsers PhantomJS",
    "watch:test": "npm run test -- --browsers PhantomJS --autoWatch=true --singleRun=false",
    "prebuild:prod": "npm run clean:dist && npm run test:once",
    "build:prod": "set NODE_ENV=production & webpack"
  },
  "dependencies": {
    "@angular/animations": "~4.0.0",
    "@angular/common": "~4.0.0",
    "@angular/compiler": "~4.0.0",
    "@angular/core": "~4.0.0",
    "@angular/forms": "~4.0.0",
    "@angular/http": "~4.0.0",
    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/node": "^7.0.10",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "codelyzer": "^3.1.2",
    "compodoc": "0.0.41",
    "css-loader": "^0.27.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.14",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.0",
    "tslint": "^5.6.0",
    "typescript": "^2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  },
  "keywords": [
    "angular",
    "ng4",
    "webpack"
  ],
  "author": "Milind J Wankhede",
  "license": "MIT"
}
